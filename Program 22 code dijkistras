#include <stdio.h>
#define INF 999
int main() {
    int n, a[10][10], dist[10], visited[10]={0}, src;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    scanf("%d",&src);
    for(int i=0;i<n;i++)
        dist[i] = INF;
    dist[src] = 0;
    for(int count=0;count<n-1;count++) {
        int u=-1, min=INF;
        for(int i=0;i<n;i++)
            if(!visited[i] && dist[i]<min) {
                min=dist[i]; u=i;
            }
        visited[u]=1;
        for(int v=0;v<n;v++)
            if(!visited[v] && a[u][v] && dist[u]+a[u][v]<dist[v])
                dist[v]=dist[u]+a[u][v];
    }
    for(int i=0;i<n;i++)
        printf("Distance to %d = %d\n", i, dist[i]);
    return 0;
}
