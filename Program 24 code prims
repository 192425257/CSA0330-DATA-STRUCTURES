#include <stdio.h>
#define INF 999
int main() {
    int n, cost[10][10];
    int visited[10] = {0};
    int edges = 0, min, u, v, totalCost = 0;
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    printf("Enter adjacency matrix:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &cost[i][j]);
    visited[0] = 1;
    printf("\nEdges in MST:\n");
    while (edges < n - 1) {
        min = INF;
        for (int i = 0; i < n; i++) {
            if (visited[i]) {
                for (int j = 0; j < n; j++) {
                    if (!visited[j] && cost[i][j] && cost[i][j] < min) {
                        min = cost[i][j];
                        u = i;
                        v = j;
                    }
                }
            }
        }
        printf("%d - %d : %d\n", u, v, min);
        totalCost += min;
        visited[v] = 1;
        edges++;
    }
printf("Total cost of MST = %d\n", totalCost);
    return 0;
}
