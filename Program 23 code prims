#include <stdio.h>
#define INF 999
int main() {
    int n, a[10][10], selected[10]={0};
    int edges=0, min, x, y;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    selected[0]=1;
    while(edges<n-1) {
        min=INF;
        for(int i=0;i<n;i++)
            if(selected[i])
                for(int j=0;j<n;j++)
                    if(!selected[j] && a[i][j] && a[i][j]<min) {
                        min=a[i][j];
                        x=i; y=j;
                    }
        printf("%d - %d : %d\n", x, y, min);
        selected[y]=1;
        edges++;
    }
    return 0;
}
